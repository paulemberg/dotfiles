[user]
    name = Paulemberg Lima Silva
    email = paulemberg@gmail.com
[color]
    diff = auto
    status = auto
    branch = auto
[color "status"]
    added = green
    changed = blue
    untracked = red 
[color "diff"]
    meta = yellow
    old = red 
    new = green
[core]
    editor = vim
[push]
    followTags = true
    default = current
[alias]
    lg1 = log --graph --abbrev-commit --decorate --date=relative --all
    lg2 = log --graph --oneline --decorate --all
    lg3 = log --graph --pretty=format:'%C(bold blue)%h%Creset - %C(yellow)%d%Creset %s %C(cyan dim)(%cr)%Creset %n' --abbrev-commit --branches

